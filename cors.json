[
    {
        "origin": ["*"],
        "method": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
        "responseHeader": [
            "Content-Type",
            "Authorization",
            "X-Requested-With",
            "Cache-Control",
            "Pragma"
        ],
        "allowedHeader": [
            "Content-Type",
            "Authorization",
            "X-Requested-With"
        ],
        "maxAgeSeconds": 3600,
        "allowCredentials": true
    }
]